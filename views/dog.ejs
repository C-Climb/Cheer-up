<% include partials/header %>
    <div class="wrapper">
        <% include partials/nav %>
        <h1>HELLO FROM THE DOG PAGE</h1>
        <div class="image--container">
            <% randDogImages.forEach((randDogImages) => { %>
                <div class="image--container__inner"><img src="<%=randDogImages.url%>" alt="<%=randDogImages.breed%>"></div>
                <div class="landing--flex__container">
                    <div><a href="<%=randDogImages.url%>" target="_blank" rel="noopener noreferrer">Source</a></div>
                    <div><a data-key="32" href="<%=nextDogImage.url%>">Next!</a></div>
                </div>
             <% }); %>
            <div class="facts--container">
                <div><h3>Dog Fact:</h3>
                <div><p><%=dogFacts%></%=dogFacts%></p>
            </div>
        </div>
    </div>
<% include partials/headerEnd %>